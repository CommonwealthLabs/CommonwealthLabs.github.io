{
  "name": "react-starteroni",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "boot": "npm install && ./node_modules/.bin/flow-typed",
    "test:ava": "./node_modules/.bin/ava --verbose && exit $?",
    "test:eslint": "./node_modules/.bin/eslint src/** && echo $?",
    "test:sass": "./node_modules/.bin/sass-lint 'src/**/*.scss' -v -q && echo $?",
    "flow": "flow",
    "dev": "webpack-dev-server --env=dev --progress --profile --colors",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors"
  },
  "pre-commit": [
    "test:ava",
    "test:eslint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sashimii/react-starteroni.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sashimii/react-starteroni/issues"
  },
  "homepage": "https://github.com/sashimii/react-starteroni#readme",
  "ava": {
    "files": [
      "src/**/*.test.js",
      "src/*.test.js"
    ],
    "source": [
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "modules": true,
    "require": [
      "babel-register"
    ],
    "babel": {
      "extends": "./babel-test-config.json"
    }
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "file-loader": "^0.10.0",
    "foundation": "^4.2.1-1",
    "foundation-sites": "^6.3.1",
    "html-webpack-plugin": "^2.28.0",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "svg-url-loader": "^2.0.1",
    "webpack-dev-server": "^2.3.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "css-loader": "^0.26.1",
    "dot-env": "0.0.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "flow-babel-webpack-plugin": "^1.1.0",
    "flow-bin": "^0.51.1",
    "flow-webpack-plugin": "^0.3.6",
    "flowtype-loader": "^0.2.2",
    "gulp": "^3.9.1",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.5.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.5.8",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.0",
    "sasslint-webpack-plugin": "^1.0.4",
    "sinon": "^2.4.1",
    "style-loader": "^0.13.1",
    "tap-nyan": "^1.1.0",
    "webpack": "^2.7.0"
  }
}
